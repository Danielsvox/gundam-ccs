# Frontend Environment Configuration
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1

# Environment
REACT_APP_ENVIRONMENT=development

# Optional: Custom API timeout (in milliseconds)
REACT_APP_API_TIMEOUT=10000

# Optional: Enable/disable features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true

# Optional: Stripe configuration (if using Stripe Elements)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Optional: Google Analytics (if using)
REACT_APP_GA_TRACKING_ID=GA_TRACKING_ID

# Optional: Sentry error tracking (if using)
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id 